# Configuration des variables d'environnement pour le déploiement

# Backend API URL
API_URL=http://68.183.71.119:8080

# Configuration pour différents environnements
# Production
PRODUCTION=true

# Configuration CORS (si nécessaire)
CORS_ORIGIN=*

# Configuration des cartes (si vous utilisez des clés API)
MAP_API_KEY=your_map_api_key_here

# Configuration des notifications (si vous utilisez des services de notification)
NOTIFICATION_SERVICE_URL=

# Configuration des analytics (si vous utilisez Google Analytics, etc.)
ANALYTICS_ID=

# Configuration des logs
LOG_LEVEL=info

# Configuration de la sécurité
SECURE_COOKIES=true
SESSION_TIMEOUT=3600000

# Configuration des uploads
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/webp

# Configuration des performances
CACHE_TTL=3600
ENABLE_COMPRESSION=true
ENABLE_GZIP=true
