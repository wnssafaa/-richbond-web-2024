{
  "version": 2,
  "builds": [
    {
      "src": "dist/richbond/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/richbond/$1"
    },
    {
      "src": "/",
      "dest": "/dist/richbond/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
